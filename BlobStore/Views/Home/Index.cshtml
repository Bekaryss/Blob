@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-xs-6 col-md-3" style="height: 400px;">
            <a href="#" class="thumbnail" data-toggle="modal" data-target="#@item.RowKey">
                <img src="@item.URLcrop" class="img-responsive" />
            </a>
        </div>

        <div class="modal fade" tabindex="-1" role="dialog" id="@item.RowKey">
            <div class="modal-dialog">
                <img src="@item.URLcore" class="img-responsive" />
            </div>
        </div>
    }
</div>